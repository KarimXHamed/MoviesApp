import UIKit

class FlashCard: UIView {
    
    @IBOutlet weak var flashCardLabel: UILabel!
    
    override init(frame: CGRect) {
        super.init(frame: frame)
        self.commonInit()
     
    }

    required init?(coder: NSCoder) {
        super.init(coder: coder)
        self.commonInit()
       
    }

    private func commonInit() {
        let bundle = Bundle.main
        if let viewToAdd = bundle.loadNibNamed("FlashCardView", owner: self, options: nil) {
            for view in viewToAdd {
                if let contentView = view as? UIView {

                    addSubview(contentView)
                    contentView.frame = self.bounds
                    
                    contentView.autoresizingMask = [.flexibleWidth, .flexibleHeight]
                    return
                }
            }
        }
    }
   
}

Error parsing JSON: typeMismatch(Swift.Array<Any>, Swift.DecodingError.Context(codingPath: [], debugDescription: "Expected to decode Array<Any> but found a dictionary instead.", underlyingError: nil))
