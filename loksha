struct LanguageModel:Encodable {
   
    let name:String
    var levels:[Level]=[]
    
    
}
struct Level:Encodable{
let name:String
    var vocabs:[lesson]=[]
    var grammers:[lesson]=[]
    var writings:[lesson]=[]
    var convos:[lesson]=[]
    
}
struct lesson:Encodable{
    let name:String
    let text:String
    var quizes:[quiz]=[]
}

struct quiz:Encodable{
    var myQuiz:[point]=[]
    let score:Int
    
}
struct point:Encodable{
let question:String
let answer:String
}
