 override func viewDidLoad() {
        super.viewDidLoad()
        
        // Call the fetchLanguageModel function here
        fetchLanguageModel { languageModel in
            if let model = languageModel {
                // Successfully fetched the language model
                print("Successfully fetched language model: \(model)")
                
                // You can update the UI, store the model, or perform other actions here
                // For example, you can store it in a property:
                self.displayLanguageModel(model)
                
            } else {
                // Failed to fetch the language model
                print("Failed to fetch language model.")
            }
        }
    }
    
    // Function to update the UI or handle the language model data
    func displayLanguageModel(_ model: LanguageModel) {
        // For now, let's just print the model to check if it's correctly fetched
        print("Language Model Name: \(model.name)")
        
        for level in model.levels {
            print("Level Name: \(level.name)")
            for vocab in level.vocabs {
                print("Vocab Name: \(vocab.name), Text: \(vocab.text)")
            }
        }
    }
}
